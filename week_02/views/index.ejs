<!doctype html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<title>Manga</title>

	<link rel="stylesheet" href="css/style.css">
</head>

<body>
	
	<header>
		<label>Search for Manga</label>
		<input type="text" placeholder="search for manga">
	</header>

	<section>
		<% for (var i =0; i < manga.length; i++) { %>
			<article>
				<h2><%=manga[i].name %></h2>
				<!-- <img class="cover" src="<%= manga[i].cover %>"> -->
			</article>
		<% } %>
	</section>
	
	

</body>
<script src="js/script.js"></script>



</html>
